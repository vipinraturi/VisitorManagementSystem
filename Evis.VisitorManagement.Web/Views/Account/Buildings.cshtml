                function set_country() {
                    var continent = document.getElementById('ddlRegion');
                    var country = document.getElementById('ddlCountry');
                    var state = document.getElementById('ddlState');
                    var city = document.getElementById('ddlCity');
                    var countryArr;
                    country.length = 0;
                    state.length = 0;
                    city.length = 0;
                    state.disabled = true;
                    city.disabled = true;
                    var region = continent.options[continent.selectedIndex].text;
                    var loc = document.getElementById('txtJobloc');
                    if (countries[region]) {
                        country.disabled = false;
                        state.disabled = false;
                        country.options[0] = new Option('--Select Country--', '');
                        countryArr = countries[region].split('|');
                        for (var i = 0; i < countryArr.length; i++)
                            //                country.listitems[i + 1] = new ListItem(countryArr[i], countryArr[i]);
                            country.options[i + 1] = new Option(countryArr[i], countryArr[i]);
                    }
                    else country.disabled = true;
                    loc.length = 0;
                    loc.disabled = true;
                }

                function set_city_state() {
                    var country = document.getElementById('ddlCountry');
                    var state = document.getElementById('ddlState');
                    var city = document.getElementById('ddlCity');
                    var city_stateArr;
                    state.length = 0;
                    city.length = 0;
                    city.disabled = true;
                    var country_Sel = country.options[country.selectedIndex].text;
                    var loc = document.getElementById('txtJobloc');
                    if (city_states[country_Sel]) {
                        state.disabled = false;
                        state.options[0] = new Option('--Select State--', '');
                        city_stateArr = city_states[country_Sel].split('|');
                        for (var i = 0; i < city_stateArr.length; i++)
                            state.options[i + 1] = new Option(city_stateArr[i], city_stateArr[i]);
                        state.options[i + 1] = new Option('Others', 'Others');
                    }
                    else state.disabled = true;
                    loc.disabled = true;
                }

                function set_city() {
                    var country = document.getElementById('ddlCountry');
                    var state = document.getElementById('ddlState');
                    var city = document.getElementById('ddlCity');
                    var city_Arr;

                    city.length = 0;
                    var loc = document.getElementById('txtJobloc');
                    var country_Sel = country.options[country.selectedIndex].text;
                    var state_Sel = state.options[state.selectedIndex].text;
                    if (state_Sel == 'Others') {
                        city.disabled = true;
                        loc.disabled = false;
                        loc.enabled = true;
                    }
                    else if (country_Sel == 'India' || country_Sel == 'United States') {
                        loc.disabled = true;
                        city.disabled = false;
                        city.options[0] = new Option('--Select City--', '');
                        city_Arr = cities[state_Sel].split('|');
                        for (var i = 0; i < city_Arr.length; i++)
                            city.options[i + 1] = new Option(city_Arr[i], city_Arr[i]);
                    }
                    else {
                        city.disabled = true;
                        loc.disabled = false;
                        loc.enabled = true;
                    }
                }




                window.onload = function () {
                    setRegions(document.getElementById('ddlRegion'));
                }


